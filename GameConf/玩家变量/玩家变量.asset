    # \u79FB\u52A8\u901F\u5EA6\u52A0\u6210\n\u52A0\u6210_\u653B\u51FB\u529B      
    # \u653B\u51FB\u529B\u52A0\u6210\n\u52A0\u6210_\u91D1\u5E01\u83B7\u53D6     #
    \u91D1\u5E01\u83B7\u53D6\u500D\u7387\n\u7528\u9014\uFF1A\u76F4\u63A5\u53C2\u4E0E\u6570\u503C\u8BA1\u7B97\uFF0C\u63D0\u4F9B\u500D\u7387\u6216\u56FA\u5B9A\u6570\u503C\u52A0\u6210\n2.
    \u89E3\u9501\u529F\u80FD\u7C7B - \u89E3\u9501_\n\u89E3\u9501_\u91CD\u751F\u6863\u4F4D    
    # \u91CD\u751F\u6863\u4F4D\u7B49\u7EA7\uFF08\u89E3\u9501\u680F\u4F4D\uFF09\n\u89E3\u9501_VIP\u529F\u80FD     
    # VIP\u529F\u80FD\u89E3\u9501\n\u89E3\u9501_\u9AD8\u7EA7\u5730\u56FE     # \u5730\u56FE\u89E3\u9501\n\u89E3\u9501_\u7279\u6B8A\u5546\u5E97    
    # \u5546\u5E97\u89E3\u9501\n\u7528\u9014\uFF1A\u63A7\u5236\u529F\u80FD\u5F00\u542F\uFF0C\u51B3\u5B9A\u73A9\u5BB6\u80FD\u4F7F\u7528\u54EA\u4E9B\u529F\u80FD\n3.
    \u72B6\u6001\u6807\u8BB0\u7C7B - \u72B6\u6001_\n\u72B6\u6001_VIP\u7B49\u7EA7     
    # VIP\u7B49\u7EA7\u72B6\u6001\n\u72B6\u6001_\u5728\u7EBF\u72B6\u6001     # \u5728\u7EBF\u79BB\u7EBF\u72B6\u6001\n\u72B6\u6001_\u6218\u6597\u6A21\u5F0F    
    # \u5F53\u524D\u6218\u6597\u6A21\u5F0F\n\u72B6\u6001_\u4EFB\u52A1\u9636\u6BB5    
    # \u4EFB\u52A1\u8FDB\u884C\u9636\u6BB5\n\u7528\u9014\uFF1A\u8BB0\u5F55\u5F53\u524D\u72B6\u6001\uFF0C\u7528\u4E8E\u6761\u4EF6\u5224\u65AD\u548C\u903B\u8F91\u5206\u652F\n4.
    \u6280\u80FD\u7B49\u7EA7\u7C7B - skill_\nskill_\u706B\u7403\u672F      # \u706B\u7403\u672F\u7B49\u7EA7\nskill_\u6CBB\u7597\u672F     
